my_list = []
for i in range (10,20,30,40):
my_list.append(i)

print(my_list)

my_list.insert(1, 15)

print(my_list)

extend_list=[50,60,70]
my_list.extend(extend_list)
print("extended list")
print(my_list)

my_list.pop(-1) 
print("remove last list")
print(my_list)

sorted(my_list)
print("sorted")
print(my_list)

print("find and print index")
print(my_list[30])
